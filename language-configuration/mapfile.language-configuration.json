{
	"wordPattern": "[\\w.~%&$:\\\\+\\/-]+",
	"brackets": [
		["[", "]"],
		["(", ")"],
		["\"", "\""]
	],
	"autoCloseBefore": ";:.,=])\n\t",
	"autoClosingPairs": [
		{ "open": "[", "close": "]" },
		{ "open": "(", "close": ")" },
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
	],
	"surroundingPairs": [
		["[", "]"],
		["(", ")"],
		["\"", "\""]
	]
}
