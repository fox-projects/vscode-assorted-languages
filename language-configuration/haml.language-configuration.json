{
	"comments": {
		"lineComment": "-#",
		"blockComment": ["/", ""]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "`"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "`"]
	],
	"indentationRules": {
		"increaseIndentPattern": "^s*(([-%#\\:\\.\\=])|(.*sdo\b))\b[^{;]*$"
	}
}
